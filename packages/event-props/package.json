  },
  "keywords": [
    "event",
    "store",
    "reactive",
    "solid",
    "primitives"
  ],
  "devDependencies": {
    "jsdom": "^18.1.1",
    "prettier": "^2.5.0",
    "solid-register": "^0.1.1",
    "tslib": "^2.3.1",
    "tsup": "^5.10.1",
    "typescript": "^4.5.2",
    "uvu": "^0.5.2"
  },
  "peerDependencies": {
    "solid-js": "^1.2.5"
  }
}
